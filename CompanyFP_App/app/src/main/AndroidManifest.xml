<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.companyfp_app">
    <!-- Permisos para Retrofit -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo300"
        android:label="CompanyFP"
        android:roundIcon="@mipmap/logo300"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" />
        <activity android:name=".DashboardActivity" />
        <activity
            android:name=".StudentsActivity"
            android:label="@string/title_activity_students"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>
</manifest>